{
    "manifest_version": 2,
    "name": "屏蔽网址",
    "version": "1.0.0",
    "options_page":"options.htm",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "19": "ico_19.png",
            "38": "ico_38.png"
        },
        "default_popup": "popup.htm",
        "default_title": "屏蔽列表中的网址，并跳转到指定网址。"
    }
}